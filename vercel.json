{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/youtube/*.ts": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  }
}
